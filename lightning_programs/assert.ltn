var lib = []

lib.assert = function(value1, value2, msg)
{
    if(value1 != value2){
        write_line(msg)
        return false
    }
    return true
}
lib.assert_not = function(value1, value2, msg)
{
    if(value1 == value2){
        write_line(msg)
        return false
    }
    return true
}

return lib