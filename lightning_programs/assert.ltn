var lib = []
lib.header = ""
lib.assert =
    function (value1, value2, msg){
        if(value1 != value2){
            write_line(lib.header .. msg)
            return true
        }
        return false
    }
lib.assert_not =
    function (value1, value2, msg){
        if(value1 == value2){            
            write_line(lib.header .. msg)
            return true
        }
        return false
    }

return lib